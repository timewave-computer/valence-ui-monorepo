{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
      "allowJs": true,
      "incremental": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "lib": ["dom", "dom.iterable", "esnext", "webworker"],
      "module": "esnext",
      "moduleResolution": "node",
      "noEmit": true,
      "resolveJsonModule": true,
      "strict": false,
      "strictNullChecks": true,
      "target": "es2020"
    }
  }